{
    "name": "Sylvanian Extention",
    "manifest_version": 2,
    "version": "0.1",
    "description": "高齢者が若者みたいにインターネットを使いこなしてもらえるようにと開発を行なっている拡張機能です。",
    "action": {
        "default_popup": "/html/index.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/jquery-3.6.0.min.js",
                "js/content.js"
            ]
        }
    ],
    "web_accessible_resources":[
        "images/*.png"
    ]
}